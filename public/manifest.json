{
  "manifest_version": 2,
  "name": "Agentium: User agent switcher",
  "version": "0.1",
  "description": "Switches the user agent string of your browser.",
  "icons": {
    "32": "icon-32.png",
    "128": "icon-128.png"
  },
  "browser_action": {
    "default_icon": {
      "32": "icon-32.png"
    },
    "default_title": "Agentium",
    "default_popup": "popup.html"
  },
  "options_page": "options.html"
}
